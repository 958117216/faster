<launch>

	<node name="scan_filter" pkg="acl_planning" type="gpf" output="screen" clear_params="true">

		<param name="ground_range" type="double" value="5.6"/>
		<param name="filter_thresh" type="double" value="0.2"/>
		<param name="new_max_range" type="double" value="1.5"/>

		<remap from="~vicon" to="state_estimate"/>
		<remap from="~scan" to="scan"/>
		<remap from="~filtered_scan" to="filtered_scan"/>	
	</node>


</launch>
